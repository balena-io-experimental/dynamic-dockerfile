
# AUTOGENERATED FILE
# Resin.io base image for Raspberry Pi 2, Fedora, node v5.12.0, variant: slim
FROM resin/raspberrypi2-fedora:latest

ENV NODE_VERSION 5.12.0

RUN curl -SLO "http://resin-packages.s3.amazonaws.com/node/v5.12.0/node-v5.12.0-linux-armv7hf.tar.gz" \
	&& echo "" | sha256sum -c - \
	&& tar -xzf "node-v5.12.0-linux-armv7hf.tar.gz" -C /usr/local --strip-components=1 \
	&& rm "node-v5.12.0-linux-armv7hf.tar.gz" \
	&& npm config set unsafe-perm true -g --unsafe-perm \
	&& rm -rf /tmp/*

CMD ["echo","'No CMD command was set in Dockerfile! Details about CMD command could be found in Dockerfile Guide section in our Docs. Here's the link: http://docs.resin.io/#/pages/using/dockerfile.md"]