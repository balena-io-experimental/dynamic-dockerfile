# Resin.io base image for Raspberry Pi 3, Fedora, node v6.3.1, variant: slim
FROM resin/raspberrypi3-fedora:latest

ENV NODE_VERSION 6.3.1

RUN curl -SLO "http://resin-packages.s3.amazonaws.com/node/v6.3.1/node-v6.3.1-linux-armv7hf.tar.gz" \
	&& echo "" | sha256sum -c - \
	&& tar -xzf "node-v6.3.1-linux-armv7hf.tar.gz" -C /usr/local --strip-components=1 \
	&& rm "node-v6.3.1-linux-armv7hf.tar.gz" \
	&& npm config set unsafe-perm true -g --unsafe-perm \
	&& rm -rf /tmp/*

CMD ["echo","'No CMD command was set in Dockerfile! Details about CMD command could be found in Dockerfile Guide section in our Docs. Here's the link: http://docs.resin.io/#/pages/using/dockerfile.md"]