# Resin.io base image for Intel Edison, Debian, node v6.3.1, variant: old_suite
FROM resin/edison-buildpack-deps:wheezy

ENV NODE_VERSION 6.3.1

RUN curl -SLO "http://nodejs.org/dist/v6.3.1/node-v6.3.1-linux-i386.tar.gz" \
	&& echo "" | sha256sum -c - \
	&& tar -xzf "node-v6.3.1-linux-i386.tar.gz" -C /usr/local --strip-components=1 \
	&& rm "node-v6.3.1-linux-i386.tar.gz" \
	&& npm install mraa@$MRAA_VERSION \
	&& npm cache clear \
	&& npm config set unsafe-perm true -g --unsafe-perm \
	&& rm -rf /tmp/*

CMD ["echo","'No CMD command was set in Dockerfile! Details about CMD command could be found in Dockerfile Guide section in our Docs. Here's the link: http://docs.resin.io/#/pages/using/dockerfile.md"]