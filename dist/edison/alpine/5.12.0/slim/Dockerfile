	&& set -x \
	&& apk add --no-cache $buildDeps python \
	&& curl -SLO "http://resin-packages.s3.amazonaws.com/node/v5.12.0/node-v5.12.0-linux-alpine-i386.tar.gz" \
	&& echo "eb49f6bb180b2ef05d723c337f996ed8a18e558d5d5c7d601728c07213037ca9  node-v5.12.0-linux-alpine-i386.tar.gz" | sha256sum -c - \
	&& tar -xzf "node-v5.12.0-linux-alpine-i386.tar.gz" -C /usr/local --strip-components=1 \
	&& rm "node-v5.12.0-linux-alpine-i386.tar.gz" \
	&& apk del $buildDep \
	&& npm install mraa \
	&& npm cache clear \
	&& npm config set unsafe-perm true -g --unsafe-perm \
	&& rm -rf /tmp/*

