FROM resin/ts7700-alpine:latest

ENV NODE_VERSION 4.4.7

# Install dependencies
RUN apk add --no-cache libgcc libstdc++ libuv

RUN buildDeps='curl' \
	&& set -x \
	&& apk add --no-cache $buildDeps \
	&& curl -SLO "http://resin-packages.s3.amazonaws.com/node/v4.4.7/node-v4.4.7-linux-UNSUPPORTED.tar.gz" \
	&& echo "" | sha256sum -c - \
	&& tar -xzf "node-v4.4.7-linux-UNSUPPORTED.tar.gz" -C /usr/local --strip-components=1 \
	&& rm "node-v4.4.7-linux-UNSUPPORTED.tar.gz" \
	&& apk del $buildDeps \
	&& npm config set unsafe-perm true -g --unsafe-perm \
	&& rm -rf /tmp/*

