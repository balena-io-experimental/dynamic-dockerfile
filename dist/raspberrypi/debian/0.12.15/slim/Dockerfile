	&& set -x \
	&& apt-get update && apt-get install -y $buildDeps --no-install-recommends \
	&& rm -rf /var/lib/apt/lists/* \
	&& curl -SLO "{ $binary_url }}" \
	&& echo "d4932adf270e6216716061356e5cdb5ca520ff6510272cf1bbbf917f0d987bcc  node-v0.12.15-linux-armv6hf.tar.gz" | sha256sum -c - \
	&& tar -xzf "node-v0.12.15-linux-armv6hf.tar.gz" -C /usr/local --strip-components=1 \
	&& rm "node-v0.12.15-linux-armv6hf.tar.gz" \
	&& apt-get purge -y --auto-remove $buildDeps \
	&& npm config set unsafe-perm true -g --unsafe-perm \
	&& rm -rf /tmp/*

