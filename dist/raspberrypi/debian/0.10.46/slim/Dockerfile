	&& set -x \
	&& apt-get update && apt-get install -y $buildDeps --no-install-recommends \
	&& rm -rf /var/lib/apt/lists/* \
	&& curl -SLO "{ $binary_url }}" \
	&& echo "85a829d5094b0529252f529b68be6f59148b1574d8624c0d45c321e75d9a405a  node-v0.10.46-linux-armv6hf.tar.gz" | sha256sum -c - \
	&& tar -xzf "node-v0.10.46-linux-armv6hf.tar.gz" -C /usr/local --strip-components=1 \
	&& rm "node-v0.10.46-linux-armv6hf.tar.gz" \
	&& apt-get purge -y --auto-remove $buildDeps \
	&& npm config set unsafe-perm true -g --unsafe-perm \
	&& rm -rf /tmp/*

