# Resin.io base image for Intel NUC, Fedora, node v0.10.22, variant: old_suite
FROM resin/nuc-fedora-buildpack-deps:23

ENV NODE_VERSION 0.10.22

RUN curl -SLO "http://nodejs.org/dist/v0.10.22/node-v0.10.22-linux-amd64.tar.gz" \
	&& echo "" | sha256sum -c - \
	&& tar -xzf "node-v0.10.22-linux-amd64.tar.gz" -C /usr/local --strip-components=1 \
	&& rm "node-v0.10.22-linux-amd64.tar.gz" \
	&& npm config set unsafe-perm true -g --unsafe-perm \
	&& rm -rf /tmp/*

CMD ["echo","'No CMD command was set in Dockerfile! Details about CMD command could be found in Dockerfile Guide section in our Docs. Here's the link: http://docs.resin.io/#/pages/using/dockerfile.md"]