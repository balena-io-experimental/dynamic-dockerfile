	&& set -x \
	&& apk add --no-cache $buildDeps \
	&& curl -SLO "http://resin-packages.s3.amazonaws.com/node/v6.3.1/node-v6.3.1-linux-alpine-amd64.tar.gz" \
	&& echo "b2490a2d0cc489b82bdced07897602f8442b324236fe580d4a7d597f46724150  node-v6.3.1-linux-alpine-amd64.tar.gz" | sha256sum -c - \
	&& tar -xzf "node-v6.3.1-linux-alpine-amd64.tar.gz" -C /usr/local --strip-components=1 \
	&& rm "node-v6.3.1-linux-alpine-amd64.tar.gz" \
	&& apk del $buildDeps \
	&& npm config set unsafe-perm true -g --unsafe-perm \
	&& rm -rf /tmp/*

