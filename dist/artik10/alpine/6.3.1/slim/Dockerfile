	&& set -x \
	&& apk add --no-cache $buildDeps \
	&& curl -SLO "http://resin-packages.s3.amazonaws.com/node/v6.3.1/node-v6.3.1-linux-alpine-armhf.tar.gz" \
	&& echo "636596f940d29748f33beabd00772d0c26df5331b73248870116261ed33944e9  node-v6.3.1-linux-alpine-armhf.tar.gz" | sha256sum -c - \
	&& tar -xzf "node-v6.3.1-linux-alpine-armhf.tar.gz" -C /usr/local --strip-components=1 \
	&& rm "node-v6.3.1-linux-alpine-armhf.tar.gz" \
	&& apk del $buildDeps \
	&& npm config set unsafe-perm true -g --unsafe-perm \
	&& rm -rf /tmp/*

