RUN curl -SLO "http://nodejs.org/dist/v5.12.0/node-v5.12.0-linux-armv7l.tar.gz" \
	&& echo "" | sha256sum -c - \
	&& tar -xzf "node-v5.12.0-linux-armv7l.tar.gz" -C /usr/local --strip-components=1 \
	&& rm "node-v5.12.0-linux-armv7l.tar.gz" \
	&& npm config set unsafe-perm true -g --unsafe-perm \
	&& rm -rf /tmp/*

