# Resin.io base image for Beaglebone Green Wireless, Fedora, node v0.10.22, variant: onbuild
FROM resin/beaglebone-green-wifi-fedora-node:0.10.22

ENV NODE_VERSION 0.10.22

RUN mkdir -p /usr/src/app
WORKDIR /usr/src/app

ONBUILD COPY package.json /usr/src/app/
ONBUILD RUN DEBIAN_FRONTEND=noninteractive JOBS=MAX npm install --unsafe-perm
ONBUILD COPY . /usr/src/app

CMD [ "npm", "start" ]